{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@xata.io/tsconfig/nextjs.json",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "paths": {
      "~/*": ["./src/*"]
    },
    "types": ["vitest/globals"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.mjs", ".next/types/**/*.ts", "images.d.ts"],
  "exclude": ["node_modules"]
}
